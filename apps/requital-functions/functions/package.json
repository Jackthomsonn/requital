{
  "name": "functions",
  "scripts": {
    "lint": "npx eslint",
    "dev": "firebase emulators:start --import=./seed --export-on-exit=./seed & tsc --watch",
    "build": "tsc",
    "serve": "npm run build && npm run emulators:start",
    "emulators:start": "firebase emulators:start --import=./seed --export-on-exit=./seed",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "grab:port": "lsof -i tcp:4000",
    "test:debug": "jest",
    "test": "jest --silent"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^3.1.0",
    "axios": "^0.27.2",
    "expo-server-sdk": "^3.6.0",
    "firebase-admin": "11.0.1",
    "firebase-functions": "^3.22.0",
    "firebase-tools": "^11.4.0",
    "moment": "^2.29.4",
    "plaid": "^10.8.0",
    "requital-converter": "*"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "firebase-functions-test": "latest",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "private": true
}
