{
  "name": "functions",
  "scripts": {
    "lint": "npx eslint",
    "dev": "firebase emulators:start --import=./seed --export-on-exit=./seed & tsc --watch",
    "build": "tsc",
    "serve": "npm run build && npm run emulators:start",
    "emulators:start": "firebase emulators:start --import=./seed --export-on-exit=./seed",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy:dev": "firebase use dev --add && firebase deploy --only functions",
    "deploy:prod": "firebase use prod --add && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "grab:port": "lsof -i tcp:4000"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "firebase-admin": "11.0.0",
    "firebase-functions": "^3.22.0",
    "firebase-tools": "^11.4.0",
    "moment": "^2.29.4",
    "plaid": "^10.8.0",
    "requital-converter": "*"
  },
  "devDependencies": {
    "firebase-functions-test": "latest",
    "typescript": "^4.7.4"
  },
  "private": true
}
